<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EBEBEB"
    android:orientation="vertical"
    tools:context=".Main_bar">

    <TextView
        android:id="@+id/text_details"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:layout_marginTop="20dp"
        android:paddingRight="20dp"
        android:paddingLeft="20dp"
        android:text="@string/text_title"
        android:textColor="@color/grey"
        android:textSize="18dp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="140dp"
        android:layout_marginHorizontal="40dp"
        android:layout_marginTop="40dp"
        android:layout_marginBottom="40dp"
        android:orientation="horizontal"
        android:weightSum="2">

        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginVertical="10dp"
            android:layout_weight="1"
            android:background="@drawable/squareshape"
            app:cardBackgroundColor="#e0e0e0"
            app:cardCornerRadius="4dp">

            <ImageView
                android:id="@+id/img_gm"
                android:layout_width="90dp"
                android:layout_height="50dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="20dp"
                android:src="@drawable/gm" />

            <TextView
                android:id="@+id/txt_gm"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignEnd="@+id/menuLayout_gm"
                android:layout_alignRight="@+id/menuLayout_gm"
                android:layout_marginTop="80dp"
                android:gravity="center"
                android:text="سند قبض نقدى"
                android:textColor="@color/grey" />
        </android.support.v7.widget.CardView>

        <Space
            android:layout_width="40dp"
            android:layout_height="match_parent" />

        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginVertical="10dp"
            android:layout_weight="1"
            app:cardBackgroundColor="#e0e0e0"
            app:cardCornerRadius="4dp">

            <ImageView
                android:id="@+id/image_gr"
                android:layout_width="90dp"
                android:layout_height="50dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="20dp"
                android:src="@drawable/gr" />

            <TextView
                android:id="@+id/txt_gr"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignEnd="@+id/menuLayout_gm"
                android:layout_alignRight="@+id/menuLayout_gm"
                android:layout_marginTop="80dp"
                android:gravity="center"
                android:text="سند قبض عيني"
                android:textColor="@color/grey" />
        </android.support.v7.widget.CardView>

    </LinearLayout>


    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:layout_marginHorizontal="40dp"
        app:cardBackgroundColor="#e0e0e0"
        app:cardCornerRadius="4dp">

        <ImageView
            android:id="@+id/img_pr"
            android:layout_width="90dp"
            android:layout_height="50dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="20dp"
            android:src="@drawable/pr" />

        <TextView
            android:id="@+id/txt_pr"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/menuLayout_gm"
            android:layout_alignRight="@+id/menuLayout_gm"
            android:layout_marginTop="80dp"
            android:gravity="center"
            android:text="المنتجات"
            android:textColor="@color/grey" />
    </android.support.v7.widget.CardView>

